  "dependencies": {
    "@inquirer/prompts": "^8.2.1",
    "dotenv": "^17.2.3",
    "openai": "^6.22.0",
    "pino": "^10.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  }
}
