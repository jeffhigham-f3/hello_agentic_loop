{
  "name": "hello-agentic-loop",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.10.3",
  "scripts": {
    "dev": "tsx src/index.ts",
    "smoke:openai": "tsx src/scripts/openaiSmoke.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
